  <!DOCTYPE html>
  <html>
      <head>
        <title>{% block title %}Sticky Notes{% endblock %}</title>
        <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
      <link rel="stylesheet" href="{{ asset('css/jquery-ui-1.9.2.custom.css') }}" type="text/css" media="all"/>
      <link rel="stylesheet" href="{{ asset('bundles/applicationmynotes/css/todo.css') }}" type="text/css" media="all"/>
</head>
       
          <body>
		
		<h1>Ma TODO-LIST</h1>

		<div class="shoppingList">
                  <ul>  
                    {% for entity in pagination %}
                      {#{{ entity.text|raw }}#}
                      {% set mypos = entity.xyz|split('x') %}
                     {% set mysize = entity.wh|split('x') %}
<!--<tr {% if loop.index is odd %}class="color"{% endif %}>-->
             {#% debug entities.categories%#}
                          <li>{{entity.text|raw}}</li>
   
    
    {% endfor %}
			
		</div>
<div class="done"></div>
	</body>

         
    {% block javascripts %}
    <script src="{{ asset('jquery/jquery-1.8.3.js') }}" type="text/javascript"></script>
    <script type="text/javascript" src="{{ asset('jquery/jquery-ui-1.9.2.custom.min.js') }}"></script>
     <script type="text/javascript" src="{{ asset('bundles/applicationmynotes/js/jquery.shoppingList.js') }}"></script>
 <script type="text/javascript" src="{{ asset('bundles/applicationmynotes/js/script.js') }}"></script>
    
{% endblock %}
    </body>

</html>
